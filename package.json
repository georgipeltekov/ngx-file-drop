{
  "repository": "georgipeltekov/ngx-file-drop",
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 6.9.0"
  },
  "scripts": {
    "clean": "rimraf out-tsc dist/*",
    "prebuild": "npm run clean",
    "build": "ng-packagr -p ./src/package.json",
    "prestart": "npm install",
    "test:watch": "karma start ./scripts/karma.conf.js --no-single-run --auto-watch",
    "test": "karma start ./scripts/karma.conf.js --single-run",
    "coveralls": "cat ./coverage/lcov/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "tslint --type-check --project ./src/tsconfig.json"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular-devkit/core": "^8.0.0",
    "@angular/cli": "^8.0.0",
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@types/node": "^12.0.3",
    "codelyzer": "^5.0.1",
    "ng-packagr": "^4.4.0",
    "ts-node": "~7.0.1",
    "tsickle": "^0.35.0",
    "tslint": "~5.16.0",
    "typescript": "~3.4.5",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "rxjs": "^6.5.2",
    "zone.js": "~0.9.1",
    "@types/jasmine": "^2.8.8",
    "@types/selenium-webdriver": "3.0.10",
    "coveralls": "^3.0.3",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.2",
    "karma-html-reporter": "^0.2.7",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7"
  }
}
